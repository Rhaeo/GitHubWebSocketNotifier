{
  "manifest_version": 2,
  "name": "GitHub WebSocket Notifier",
  "description" : "Listens to GitHub notifications WebSocket and downloads the notification count.",
  "version": "1.0.0",
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "GitHub WebSocket Notifier",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "https://github.com/notifications"
  ]
}
